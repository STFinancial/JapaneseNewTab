{
	"name": "Japanese Vocab Extension",
	"version": "1.2.0",
	"description": "Takes a random JLPT N5 vocab word from jlearn.net",
	"manifest_version": 2,
	"chrome_url_overrides": {
		"newtab": "newtab.html"
	},
	"content_scripts": [{
		"matches": ["<all_urls>", "http://*/*"],
		"js": ["jquery.js", "script.js", "options.js"],
		"css": ["styles.css", "options.css"]
	}],
	"options_ui": {
		"page": "options.html",
		"open_in_tab": false
	},
	"permissions": ["storage"]
}